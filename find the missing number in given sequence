import java.util.*;
class Main {
	public static void main(String[]args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int miss=1;
		int arr[]=new int[a];
		for(int i=0; i<a; i++) {
			arr[i]=sc.nextInt();

			if(arr[i]<=0) {
				continue;
			}
			else if(arr[i]==miss) {
				miss++;

			}
			else if(arr[i]>miss) {
				break;
			}
		}
		System.out.println(miss);
	}

}
