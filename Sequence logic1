import java.util.*;
public class Main
{
	public static void main(String[] args) {
		System.out.println("enter the number");
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=1;i<=n;i++){
		    if(i%2==0){
		        int res=(i*i)-2;
		        System.out.print(res);
		    }else{
		        int res=(i*i)-1;
		        System.out.print(res);
		    }
		}
	}
}
