import java.util.Scanner;
class Multiply{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        for(int i=1;i<=10;i++){
            for(int j=1;j<=10;j++){
                System.out.print(i*j +"\t");
            }System.out.println(" ");
        }

    }}
