import java.util.*;
public class Main
{
	public static void main(String[] args) {
	 int temp=0;
	 int a=1;
	 Scanner sc=new Scanner(System.in);
	 int b=sc.nextInt();
	 System.out.print(temp+" "+a);
	 for(int i=0;i<=b;i++){
	     int c=temp+a;
	     temp=a;
	     a=c;
	     
	     System.out.print(" "+c);
	 }
	}
}
